<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.1.xsd"
>

	<bean class="com.autowire.Department" name="department">
	<property name="name" value="IT Department"></property> 
	</bean>
	
	<bean class="com.autowire.Employee" name="e1" autowire="byName">
	<property name="id" value="1"></property>
	<property name="name" value="Bhushan"></property>
	<property name="company" value="Yash"></property>
	<property name="salary" value="1000000"></property>
	</bean>

	
	<bean class="com.autowire.Employee" name="e2" autowire="byName">
	<property name="id" value="2"></property>
	<property name="name" value="Rahul"></property>
	<property name="company" value="Yash"></property>
	<property name="salary" value="70000"></property>
	</bean>

		<bean class="com.autowire.Employee" name="e3" autowire="byName">
	<property name="id" value="3"></property>
	<property name="name" value="Shubham"></property>
	<property name="company" value="Yash"></property>
	<property name="salary" value="60000"></property>
	</bean>
	
		<bean class="com.autowire.Employee" name="e4" autowire="byName">
	<property name="id" value="4"></property>
	<property name="name" value="Nayan"></property>
	<property name="company" value="Yash"></property>
	<property name="salary" value="45000"></property>
	</bean>
	
		<bean class="com.autowire.Employee" name="e5" autowire="byName">
	<property name="id" value="5"></property>
	<property name="name" value="ALLU"></property>
	<property name="company" value="Yash"></property>
	<property name="salary" value="30000"></property>
	</bean>
</beans>